    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
